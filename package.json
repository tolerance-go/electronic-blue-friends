{
   "name": "my-app",
   "version": "0.1.0",
   "private": true,
   "scripts": {
      "build": "next build",
      "db:dev": "pnpm db:studio",
      "db:format": "prisma format",
      "db:generate": "prisma generate",
      "db:migrate:deploy": "prisma migrate deploy",
      "db:migrate:dev": "prisma migrate dev",
      "db:push": "prisma db push --skip-generate && pnpm db:gen",
      "db:seed": "tsx scripts/seed.ts",
      "db:studio": "prisma studio",
      "dev": "next dev -p 3004",
      "format": "prettier '**/*.{css,js,mjs,ts,tsx,md,mdx}' --write",
      "postinstall": "pnpm db:generate",
      "lint": "prettier '**/*.{css,js,mjs,ts,tsx,md,mdx}' --check && next lint",
      "start": "next start -p 3004",
      "test": "vitest run",
      "test:dev": "vitest -u"
   },
   "dependencies": {
      "@headlessui/react": "^1.7.8",
      "@next/font": "13.1.6",
      "@types/node": "18.11.18",
      "@types/react": "18.0.27",
      "@types/react-dom": "18.0.10",
      "clsx": "^1.2.1",
      "dayjs": "^1.11.7",
      "eslint": "8.33.0",
      "eslint-config-next": "13.1.6",
      "immer": "^9.0.19",
      "invariant": "^2.2.4",
      "lodash-es": "^4.17.21",
      "next": "13.1.6",
      "prisma": "^4.9.0",
      "react": "18.2.0",
      "react-cookie": "^4.1.1",
      "react-dom": "18.2.0",
      "react-slick": "^0.29.0",
      "react-use": "^17.4.0",
      "rmc-picker": "^5.0.10",
      "slick-carousel": "^1.8.1",
      "store2": "^2.14.2",
      "typescript": "4.9.5",
      "uid": "^2.0.1",
      "universal-cookie": "^4.0.4",
      "zustand": "^4.3.2"
   },
   "devDependencies": {
      "@prisma/client": "^4.9.0",
      "@types/invariant": "^2.2.35",
      "@types/lodash-es": "^4.17.6",
      "@types/react-slick": "^0.23.10",
      "autoprefixer": "^10.4.13",
